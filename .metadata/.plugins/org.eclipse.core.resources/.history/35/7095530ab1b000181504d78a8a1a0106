spring: 
  servlet: 
    multipart: 
      max-file-size: 10Mb     #最大支持文件大小
      max-request-size: 10Mb  #最大支持请求大小
      enabled: true          #默认支持温家安上传
      file-size-threshold: 0 #支持文件写入磁盘
  devtools:
    restart:
      enabled: true
      
server:
  port: 8081
  servlet:
    context-path: /uploadfile
    
logging:
  level:
    root: info
    