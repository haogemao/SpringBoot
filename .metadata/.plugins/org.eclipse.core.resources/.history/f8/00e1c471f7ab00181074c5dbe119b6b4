server:
  port: 8081
  
  
spring:
  jpa:
    properties:
      hibernate:
        show_sql: true
        
    hibernate:
      ddl-auto: update
        
  datasource:
    driver-class-name: com.mysql.jdbc.Driver
    url: jdbc:mysql://localhost:3306/springboot?useUnicode=true&characterEncoding=UTF-8&useSSL=false
    username: root
    password: root
    
  application:
    name: spring_boot_cache
    