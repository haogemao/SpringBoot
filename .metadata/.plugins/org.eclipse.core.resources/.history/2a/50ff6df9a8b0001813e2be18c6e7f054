spring: 
  servlet: 
    multipart: 
      max-file-size: 10M     #最大支持文件大小
      max-request-size: 10M
      enabled: true
      file-size-threshold: 0
  devtools:
    restart:
      enabled: true
      
server:
  port: 8081
  servlet:
    context-path: /upload
    
logging:
  level:
    root: debug
    